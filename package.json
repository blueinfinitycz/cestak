  },
  "scripts": {
    "start": "env-cmd -f .env.localdev react-scripts start",
    "build": "react-scripts build",
    "build-dev": "env-cmd -f .env.dev react-scripts build",
    "build-prod": "env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "set-api-generator": "openapi-generator-  cli version-manager set 5.3.0",
    "generate-api": "yarn run set-api-generator & npx @openapitools/openapi-generator-cli generate -i src/api/swagger.yaml -g typescript-axios -o src/api/generated"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },  
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"  
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "redux-devtools-extension": "^2.13.9"
